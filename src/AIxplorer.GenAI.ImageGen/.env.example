# .env.example

# Rename this file to .env and modify the values to suit your environment.

# Model configuration
# Path to the Stable Diffusion model directory (adjust the path accordingly)
MODEL_PATH=/path/to/stable-diffusion-3.5-medium

# FastAPI configuration
# Environment setting for the application (e.g., development, production)
APP_ENV=development

# Enable or disable debug mode
DEBUG=True

# Server configuration
# Host to bind the FastAPI server to (0.0.0.0 for all interfaces)
SERVER_HOST=0.0.0.0

# Port on which the server will run
SERVER_PORT=7600
